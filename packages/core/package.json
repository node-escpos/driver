  "repository": {
    "type": "git",
    "url": "git+https://github.com/dohooo/[pkg-name].git"
  },
  "bugs": "https://github.com/dohooo/[pkg-name]/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
